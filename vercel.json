{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/:match*",
      "destination": "https://YOUR-BACKEND-HOST/:match*"
    },
    {
      "source": "/ws/:match*",
      "destination": "https://YOUR-WS-HOST/:match*"
    }
  ]
}


